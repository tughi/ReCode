write :: (file: @FILE, chars: @Int8) -> @FILE {
    index := 0
    loop {
        char := chars[index]
        if (char == '\0') {
            return file
        }
        file.write(char)
        index = 1 + index
    }
}

write :: (file: @FILE, char: Int8) -> @FILE {
    fputc(char as Int32, file)
    return file
}

FILE :: struct
stdout: @FILE = external

fputc :: (char: Int32, file: @FILE) -> Int32
