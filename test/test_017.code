putchar :: (char: Int) -> Nothing

print :: () -> Nothing {
    digits: [Int; 20]
    digits_length: Int
    digits_length = 0
    loop {
        digits[digits_length] = print__number // 10 + '0' as Int
        digits_length = digits_length + 1
        print__number = print__number / 10
        if (print__number == 0) {
            break
        }
    }
    loop {
        if (digits_length > 0) {
            digits_length = digits_length - 1
            putchar(digits[digits_length])
        } else {
            break
        }
    }
    putchar('\n' as Int)
}
