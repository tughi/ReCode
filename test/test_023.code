\ Read file

FILE :: struct

fopen :: (file: @Int8, mode: @Int8) -> @FILE

fgetc :: (file: @FILE) -> Int32

putchar :: (char: Int32) -> Nothing

main :: () -> Int {
    file: @FILE = fopen("../test/test_023.code", "r")
    loop {
        char: Int32 = fgetc(file)
        if (char as Int < 0) {
            break
        }
        putchar(char)
    }
    return 0
}
