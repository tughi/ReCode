\ Test global array operations

putchar :: (char: Int) -> Nothing

number: Int
digits_length: Int
digits: [Int; 10]

main :: () -> Int {
    number = 1234567890
    digits_length = 0
    loop {
        digits[digits_length] = number // 10 + 48
        digits_length = digits_length + 1
        number = number / 10
        if (number <= 0) {
            break
        }
    }
    loop {
        if (digits_length <= 0) {
            break
        }
        digits_length = digits_length - 1
        putchar(digits[digits_length])
    }
    putchar(10)
    return 0
}
