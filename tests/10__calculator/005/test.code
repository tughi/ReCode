func main(argc: i32, argv: [[u8; ?]; ?]) {
    let i = 1
    loop {
        let arg = argv[i]
        let j = 0
        loop {
            if arg[j] == 0 {
                break
            }
            fputc(arg[j], stdout)
            j = j + 1
        }
        if i == argc - 1 {
            break
        }
        fputc(' ', stdout)
        i = i + 1
    }
}

\ ------------ LibC ------------

external type FILE

external stdin: @FILE
external stdout: @FILE
external stderr: @FILE

external func fputc(c: u8, file: @FILE) -> i32

external func exit(code: i32)
