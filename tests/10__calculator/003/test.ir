$main(%argc: i32, %argv: ptr<ptr<u8>>) {
@1:
  [ %argv %argc ]
  %argc.ptr: ptr<i32> = alloc i32
  [ %argc.ptr %argv %argc ]
  store %argc.ptr %argc
  [ %argc.ptr %argv ]
  %argv.ptr: ptr<ptr<ptr<u8>>> = alloc ptr<ptr<u8>>
  [ %argc.ptr %argv.ptr %argv ]
  store %argv.ptr %argv
  [ %argc.ptr ]
  %argc.1: i32 = load %argc.ptr
  [ %argc.1 ]
  %1: i32 = const 1
  [ %argc.1 %1 ]
  %2: bool = cmp_eq %argc.1 %1
  [ %2 ]
  br %2 @2 @3
  [ ]
@2:
  [ ]
  %3: i32 = const 1
  [ %3 ]
  call $exit %3
  [ ]
@3:
  [ ]
  ret
  [ ]
}

$exit(%code: i32)
