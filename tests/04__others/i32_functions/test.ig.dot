graph {
   fontname=monospace
   layout=fdp

   subgraph cluster_0 {
      label="$main"
      "$main%1" [label="%1"]
      "$main%1" -- "$main%2"
      "$main%2" [label="%2"]
      "$main%3" [label="%3"]
      "$main%4" [label="%4"]
      "$main%5" [label="%5"]
   }

   subgraph cluster_1 {
      label="$get_answer"
      "$get_answer%1" [label="%1"]
      "$get_answer%2" [label="%2"]
   }

   subgraph cluster_2 {
      label="$echo_value"
      "$echo_value%value" [label="%value"]
   }
}
