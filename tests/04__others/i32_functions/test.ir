$main(): i32 {
@1:
   %1: i32 = call $get_answer
   %2: i32 = const 42
   %3: bool = cmp_ne %1 %2
   br %3 @2 @3
@2:
   %4: i32 = const 1
   ret %4
@3:
   %5: i32 = const 0
   ret %5
}

$get_answer(): i32 {
@1:
   %1: i32 = const 42
   %2: i32 = call $echo_value %1
   ret %2
}

$echo_value(%value: i32): i32 {
@1:
   ret %value
}
