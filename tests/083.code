struct Test {
    result = 42
    value: i32
}

struct ExtendedTest: Test(value = 13) {
    value: i32
}

func main() -> i32 {
    let test = make ExtendedTest(value = 42)

    if test.result != 42 {
        return 1
    }

    if test.value != 42 {
        return 2
    }

    test.value = 0
    if test.value != 0 {
        return 3
    }

    if test.super.value != 13 {
        return 4
    }

    test.super.value = 42
    if test.super.value != 42 {
        return 5
    }

    let test_ref = @test
    test_ref.super.value = 13
    if test_ref.super.value != 13 {
        return 6
    }

    if test.value != 0 {
        return 7
    }

    return 0
}
