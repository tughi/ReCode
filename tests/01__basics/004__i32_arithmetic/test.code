func main() -> i32 {
    return sub(
        mul(
            add(
                div(42, 10)
                2
            )
            add(
                mod(42, 10)
                5
            )
        )
        42
    )
}

func add(v1: i32, v2: i32) -> i32 {
    return v1 + v2
}

func sub(v1: i32, v2: i32) -> i32 {
    return v1 - v2
}

func mul(v1: i32, v2: i32) -> i32 {
    return v1 * v2
}

func div(v1: i32, v2: i32) -> i32 {
    return v1 / v2
}

func mod(v1: i32, v2: i32) -> i32 {
    return v1 // v2
}
