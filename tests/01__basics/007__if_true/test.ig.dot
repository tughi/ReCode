graph {
   fontname=monospace
   layout=fdp

   subgraph cluster_0 {
      label="$main"
      "$main%result.ptr" [label="%result.ptr"]
      "$main%result.ptr" -- "$main%1"
      "$main%result.ptr" -- "$main%2"
      "$main%result.ptr" -- "$main%3"
      "$main%1" [label="%1"]
      "$main%2" [label="%2"]
      "$main%3" [label="%3"]
      "$main%result.1" [label="%result.1"]
   }
}
