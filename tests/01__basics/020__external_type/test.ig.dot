graph {
   fontname=monospace
   layout=fdp

   subgraph cluster_0 {
      label="$fputc"
   }

   subgraph cluster_1 {
      label="$main"
      "$main$stdout" [label="$stdout"]
      "$main$stdout" -- "$main%1"
      "$main$stdout" -- "$main%2"
      "$main$stdout" -- "$main%4"
      "$main$stdout" -- "$main%5"
      "$main$stdout" -- "$main%7"
      "$main%1" [label="%1"]
      "$main%1" -- "$main%2"
      "$main%2" [label="%2"]
      "$main%4" [label="%4"]
      "$main%4" -- "$main%5"
      "$main%5" [label="%5"]
      "$main%7" [label="%7"]
      "$main%7" -- "$main%8"
      "$main%8" [label="%8"]
   }
}
