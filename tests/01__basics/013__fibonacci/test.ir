$main(): i32 {
@1:
   %1: i32 = const 12
   %2: i32 = call $fibonacci %1
   %3: i32 = const 144
   %4: i32 = sub %2 %3
   ret %4
}

$fibonacci(%n: i32): i32 {
@1:
   %1: i32 = const 1
   %2: bool = cmp_le %n %1
   br %2 @2 @3
@2:
   ret %n
@3:
   %3: i32 = const 1
   %4: i32 = sub %n %3
   %5: i32 = call $fibonacci %4
   %6: i32 = const 2
   %7: i32 = sub %n %6
   %8: i32 = call $fibonacci %7
   %9: i32 = add %5 %8
   ret %9
}
