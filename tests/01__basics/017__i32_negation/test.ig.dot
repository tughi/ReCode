graph {
   fontname=monospace
   layout=fdp

   subgraph cluster_0 {
      label="$main"
      "$main%value.ptr" [label="%value.ptr"]
      "$main%value.ptr" -- "$main%1"
      "$main%value.ptr" -- "$main%value.1"
      "$main%value.ptr" -- "$main%2"
      "$main%value.ptr" -- "$main%3"
      "$main%1" [label="%1"]
      "$main%value.1" [label="%value.1"]
      "$main%value.1" -- "$main%2"
      "$main%2" [label="%2"]
      "$main%3" [label="%3"]
      "$main%3" -- "$main%value.2"
      "$main%3" -- "$main%4"
      "$main%value.2" [label="%value.2"]
      "$main%4" [label="%4"]
      "$main%5" [label="%5"]
      "$main%6" [label="%6"]
      "$main%7" [label="%7"]
   }
}
