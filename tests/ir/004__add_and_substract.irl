$main(): i32 {
@1:
   [ ]
   %1: i32 = const 1
   [ %1 ]
   %2: i32 = const 2
   [ %1 %2 ]
   %3: i32 = call $add %1 %2
   [ %3 ]
   %4: i32 = const 3
   [ %3 %4 ]
   %5: i32 = sub %3 %4
   [ %5 ]
   ret %5
}

$add(%v1: i32, %v2: i32): i32 {
@1:
   [ %v1 %v2 ]
   %1: i32 = add %v1 %v2
   [ %1 ]
   ret %1
}
