graph {
   fontname=monospace
   layout=fdp

   subgraph cluster_0 {
      label="$main"
      "$main%1" [label="%1"]
      "$main%2" [label="%2"]
      "$main%2" -- "$main%3"
      "$main%3" [label="%3"]
      "$main%4" [label="%4"]
   }

   subgraph cluster_1 {
      label="$fibonacci"
      "$fibonacci%n" [label="%n"]
      "$fibonacci%n" -- "$fibonacci%1"
      "$fibonacci%n" -- "$fibonacci%2"
      "$fibonacci%n" -- "$fibonacci%3"
      "$fibonacci%n" -- "$fibonacci%4"
      "$fibonacci%n" -- "$fibonacci%5"
      "$fibonacci%n" -- "$fibonacci%6"
      "$fibonacci%1" [label="%1"]
      "$fibonacci%2" [label="%2"]
      "$fibonacci%3" [label="%3"]
      "$fibonacci%4" [label="%4"]
      "$fibonacci%5" [label="%5"]
      "$fibonacci%5" -- "$fibonacci%6"
      "$fibonacci%5" -- "$fibonacci%7"
      "$fibonacci%5" -- "$fibonacci%8"
      "$fibonacci%6" [label="%6"]
      "$fibonacci%7" [label="%7"]
      "$fibonacci%8" [label="%8"]
      "$fibonacci%9" [label="%9"]
   }
}
