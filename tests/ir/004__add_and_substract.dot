digraph {
   fontname=monospace
   node [shape=record]

   subgraph cluster_0 {
      label="$main(): i32"

      "$main@1" [label="@1|%1: i32 = const 1\l%2: i32 = const 2\l%3: i32 = call $add %1 %2\l%4: i32 = const 3\l%5: i32 = sub %3 %4\lret %5\l"]
   }

   subgraph cluster_1 {
      label="$add(%v1: i32, %v2: i32): i32"

      "$add@1" [label="@1|%1: i32 = add %v1 %v2\lret %1\l"]
   }
}
