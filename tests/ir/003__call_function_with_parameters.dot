digraph {
   node [shape=record,width=5.5,fontname=monospace]

   subgraph {
      "$main(): i32" -> "$main@1"

      "$main@1" [label="@1|%1: i32 = const 0\l%2: i32 = call $echo %1\lret %2\l"]
   }

   subgraph {
      "$echo(%value: i32): i32" -> "$echo@1"

      "$echo@1" [label="@1|ret %value\l"]
   }
}
