type Line = struct {
  p1: Point
  p2: Point
}

type Point = struct {
  x: i32
  y: i32
}

$main(): i32 {
@1:
   [ ]
   %line.ptr: ptr<Line> = alloc Line
   [ %line.ptr ]
   %1: i32 = const 16
   [ %line.ptr %1 ]
   %2: i32 = const 32
   [ %line.ptr %2 %1 ]
   %3: Point = struct { Point.x: %1, Point.y: %2 }
   [ %line.ptr %2 %1 %3 ]
   %4: i32 = const 48
   [ %4 %line.ptr %2 %1 %3 ]
   %5: i32 = const 64
   [ %5 %4 %line.ptr %2 %1 %3 ]
   %6: Point = struct { Point.x: %4, Point.y: %5 }
   [ %5 %4 %line.ptr %2 %1 %3 %6 ]
   %7: Line = struct { Line.p1: %3, Line.p2: %6 }
   [ %5 %4 %line.ptr %2 %1 ]
   %8: ptr<Point> = offset %line.ptr Line.p1
   [ %5 %4 %line.ptr %2 %8 %1 ]
   %9: ptr<i32> = offset %8 Point.x
   [ %5 %4 %line.ptr %2 %8 %9 %1 ]
   store %9 %1
   [ %5 %4 %line.ptr %2 %8 ]
   %10: ptr<i32> = offset %8 Point.y
   [ %5 %4 %line.ptr %10 %2 ]
   store %10 %2
   [ %5 %4 %line.ptr ]
   %11: ptr<Point> = offset %line.ptr Line.p2
   [ %5 %11 %4 ]
   %12: ptr<i32> = offset %11 Point.x
   [ %5 %11 %12 %4 ]
   store %12 %4
   [ %5 %11 ]
   %13: ptr<i32> = offset %11 Point.y
   [ %13 %5 ]
   store %13 %5
   [ ]
   %14: i32 = const 0
   [ %14 ]
   ret %14
   [ ]
}
