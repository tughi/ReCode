\ Expect:
\ - output: ''

struct Token {
    line = 0u32
}

struct Integer_Token: Token {
    value: i32
}

func main() -> i32 {
    let token = make Integer_Token(value = 42)
    let token_ref = @token as @Token

    if (token_ref as @Integer_Token).value != 42 {
        return 1
    }

    return (0 as i16) as i32
}
