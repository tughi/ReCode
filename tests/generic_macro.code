\ Expect:
\ - output: ''

macro repeat[T](times: T, do: macro (step: T)) {
    let i: T = 0
    while i < times {
        do(step = i)
        i = i + 1
    }
}

func main() -> i32 {
    let sum = 1

    repeat(10u8) {
        sum = sum + (step as i32)
    }

    if sum != 46 {
        return 1
    }

    return 0
}
