type Point = struct {
  x: i32
  y: i32
}

$main(): i32 {
@1:
   [ ]
   %point.ptr: ptr<Point> = alloc Point
   [ %point.ptr ]
   %1: i32 = const 16
   [ %point.ptr %1 ]
   %2: i32 = const 32
   [ %point.ptr %2 %1 ]
   %3: Point = struct { Point.x: %1, Point.y: %2 }
   [ %point.ptr %2 %1 ]
   %4: ptr<i32> = offset %point.ptr Point.x
   [ %point.ptr %2 %4 %1 ]
   store %4 %1
   [ %point.ptr %2 ]
   %5: ptr<i32> = offset %point.ptr Point.y
   [ %point.ptr %5 %2 ]
   store %5 %2
   [ %point.ptr ]
   %6: ptr<i32> = offset %point.ptr Point.x
   [ %6 ]
   %7: i32 = load %6
   [ %7 ]
   %8: i32 = const 16
   [ %7 %8 ]
   %9: i32 = sub %7 %8
   [ %9 ]
   ret %9
   [ ]
}
